                                    <WCore-label asp-for="EnableJsBundling" />
                                    <div class="col-lg-10 col-md-6 col-sm-12">
                                        <div class="checkbox-inline">
                                            <label class="checkbox pt-3">
                                                <input asp-for="EnableJsBundling" />
                                                <span></span>
                                            </label>
                                        </div>
                                        <span asp-validation-for="EnableJsBundling"></span>
                                    </div>
                                </div>
                                <div class="form-group row ">
                                    <WCore-label asp-for="EnableCssBundling" />
                                    <div class="col-lg-10 col-md-6 col-sm-12">
                                        <div class="checkbox-inline">
