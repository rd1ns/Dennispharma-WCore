                                        <div class="checkbox-inline">
                                            <label class="checkbox pt-3">
                                                <input asp-for="SubjectFieldOnContactUsForm" />
                                                <span></span>
                                            </label>
                                        </div>
                                        <span asp-validation-for="SubjectFieldOnContactUsForm"></span>
                                    </div>
                                </div>
                                <div class="form-group row ">
                                    <WCore-label asp-for="UseSystemEmailForContactUsForm" />
                                    <div class="col-lg-10 col-md-6 col-sm-12">
                                        <div class="checkbox-inline">
                                            <label class="checkbox pt-3">
                                                <input asp-for="UseSystemEmailForContactUsForm" />
                                                <span></span>
                                            </label>
                                        </div>
                                        <span asp-validation-for="UseSystemEmailForContactUsForm"></span>
                                    </div>
                                </div>
                                <div class="form-group row ">
                                    <WCore-label asp-for="UseStoredProcedureForLoadingCategories" />
                                    <div class="col-lg-10 col-md-6 col-sm-12">
                                        <div class="checkbox-inline">
                                            <label class="checkbox pt-3">
                                                <input asp-for="UseStoredProcedureForLoadingCategories" />
                                                <span></span>
                                            </label>
                                        </div>
                                        <span asp-validation-for="UseStoredProcedureForLoadingCategories"></span>
                                    </div>
                                </div>
                                <div class="form-group row ">
                                    <WCore-label asp-for="DisplayJavaScriptDisabledWarning" />
                                    <div class="col-lg-10 col-md-6 col-sm-12">
                                        <div class="checkbox-inline">
                                            <label class="checkbox pt-3">
                                                <input asp-for="DisplayJavaScriptDisabledWarning" />
                                                <span></span>
                                            </label>
                                        </div>
                                        <span asp-validation-for="DisplayJavaScriptDisabledWarning"></span>
                                    </div>
                                </div>
                                <div class="form-group row ">
                                    <WCore-label asp-for="UseFullTextSearch" />
                                    <div class="col-lg-10 col-md-6 col-sm-12">
                                        <div class="checkbox-inline">
                                            <label class="checkbox pt-3">
                                                <input asp-for="UseFullTextSearch" />
